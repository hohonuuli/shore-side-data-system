<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	version="2.4">
	<distributable />

	<servlet>
		<servlet-name>GetOriginalDataServlet</servlet-name>
		<servlet-class>moos.ssds.services.servlet.data.GetOriginalDataServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetOriginalDataServlet</servlet-name>
		<url-pattern>/GetOriginalData/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetOriginalDataServlet</servlet-name>
		<url-pattern>*.GetOriginalData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetOriginalDataServlet</servlet-name>
		<url-pattern>/GetOriginalDataServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>DataAccessServlet</servlet-name>
		<servlet-class>moos.ssds.services.servlet.data.DataAccessServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DataAccessServlet</servlet-name>
		<url-pattern>/DataAccessServlet/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DataAccessServlet</servlet-name>
		<url-pattern>*.DataAccessServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DataAccessServlet</servlet-name>
		<url-pattern>/DataAccessServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>SensorMLConverterServlet</servlet-name>
		<servlet-class>moos.ssds.services.servlet.metadata.SensorMLConverterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SensorMLConverterServlet</servlet-name>
		<url-pattern>/SensorMLConverterServlet/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SensorMLConverterServlet</servlet-name>
		<url-pattern>*.SensorMLConverterServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SensorMLConverterServlet</servlet-name>
		<url-pattern>/SensorMLConverterServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MetadataAccessServlet</servlet-name>
		<servlet-class>moos.ssds.services.servlet.metadata.MetadataAccessServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MetadataAccessServlet</servlet-name>
		<url-pattern>/MetadataAccessServlet/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MetadataAccessServlet</servlet-name>
		<url-pattern>*.MetadataAccessServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MetadataAccessServlet</servlet-name>
		<url-pattern>/MetadataAccessServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GoogleMapsGPSData</servlet-name>
		<servlet-class>moos.ssds.services.servlet.ajax.GoogleMapsGPSDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoogleMapsGPSData</servlet-name>
		<url-pattern>/ajax/GoogleMapsGPSData</url-pattern>
	</servlet-mapping>

	<!-- JFree Chart Servlet Mappings -->
	<servlet>
		<servlet-name>DisplayChart</servlet-name>
		<servlet-class>org.jfree.chart.servlet.DisplayChart</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayChart</servlet-name>
		<url-pattern>/DisplayChart</url-pattern>
	</servlet-mapping>
</web-app>
